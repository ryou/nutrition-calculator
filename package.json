{
  "name": "nutrition-calculator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "updatePath": "pathpida --ignorePath .gitignore --output ./src/.pathpida",
    "dev:next": "next dev",
    "dev:path": "npm run updatePath -- --watch",
    "dev": "run-p dev:*",
    "build:next": "next build",
    "build:export": "next export",
    "build": "run-s updatePath build:next build:export",
    "start": "next start",
    "storybook": "start-storybook -s public -p 6006",
    "build-storybook": "build-storybook -s public",
    "typecheck": "tsc --noEmit",
    "lint:eslint": "next lint",
    "lint:format": "prettier --check \"{**/*,*}.{js,ts,jsx,tsx,css}\"",
    "lint": "run-s lint:*",
    "test": "jest",
    "check": "run-s lint typecheck test"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "apexcharts": "^3.35.0",
    "axios": "^0.27.1",
    "classnames": "^2.3.1",
    "daisyui": "^2.14.2",
    "dexie": "^3.2.2",
    "immer": "^9.0.14",
    "msw": "^0.39.2",
    "next": "12.1.5",
    "react": "17.0.2",
    "react-apexcharts": "^1.4.0",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.31.1",
    "react-query": "^3.38.0",
    "use-debounce": "^8.0.1",
    "uuid": "^8.3.2",
    "zod": "^3.15.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^27.5.0",
    "@types/node": "17.0.27",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.5",
    "babel-loader": "^8.2.5",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ban": "^1.6.0",
    "eslint-plugin-storybook": "^0.5.11",
    "eslint-plugin-strict-dependencies": "^1.0.1",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.18.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.3"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
